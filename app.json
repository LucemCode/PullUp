{
  "expo": {
    "name": "PullUp",
    "description": "A small Blog App.",
    "slug": "PullUp",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#3B193E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucemcode.pullup"
    },
    "android": {
      "package": "com.lucemcode.pullup"
    }
  }
}
